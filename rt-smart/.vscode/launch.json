{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "启动调试",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceRoot}/userapps/root/bin/hello.elf",
            "args": [],
            "cwd": "${workspaceRoot}/kernel/bsp/qemu-vexpress-a9",
            "environment": [],
            "externalConsole": true,
            "miDebuggerServerAddress": "localhost:1234",
            "serverLaunchTimeout": 2000,
            "targetArchitecture": "ARM",
            "MIMode": "gdb",
             "miDebuggerPath": "${workspaceRoot}/xpack-arm-none-eabi-gcc-14.2.1-1.1/bin/arm-none-eabi-gdb-py3.exe",
            "customLaunchSetupCommands": [],
            "launchCompleteCommand": "exec-run",
            "setupCommands": [
            {
                "text": "-enable-pretty-printing"
            },
            {
                "description": "Load STL printers",
                "text": "python import sys; sys.path.append(r'${workspaceRoot}/xpack-arm-none-eabi-gcc-14.2.1-1.1/share/gcc-14.2.1/python'.replace('\\\\', '/')); from libstdcxx.v6.printers import register_libstdcxx_printers; register_libstdcxx_printers(None)"
            },
            {
                "text": "set print pretty on"
            }
            ]
        }
    ]
}